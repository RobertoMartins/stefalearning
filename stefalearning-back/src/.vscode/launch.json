  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Blockchain - Launch via NPM",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["start"],
      "port": 9000,
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "node",
      "request": "attach",
      "protocol": "inspector",
      "restart": true,
      "port": 9000,
      "name": "Blockchain - Attach",
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
